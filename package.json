  "scripts": {
    "build:dev":      "npm run-script clean && webpack --config webpack.dev.js",
    "build:prod":     "npm run-script clean && webpack --config webpack.prod.js",
    "serve:dev":      "webpack-dev-server --config webpack.dev.js --watch",
    "serve:dev:dash": "webpack-dashboard --color 'bright-red' --title='Webpack Dashboard: Dev Server' -- webpack --config webpack.dev.js --watch",
    "serve:prod":     "webpack-dev-server --config webpack.prod.js --open",
    "serve:prod:dash": "webpack-dashboard --color 'bright-blue'  --title='Webpack Dashboard: Prod Server' -- node server.js",
    "start:dev":      "npm run-script clean && npm run-script build:dev && npm run-script serve:dev:dash",
    "test":           "echo \"Error: no test specified\" && exit 1",
    "debug":          "nodemon --inspect dev-server.js",
    "clean":          "rimraf dist/*"
  }
